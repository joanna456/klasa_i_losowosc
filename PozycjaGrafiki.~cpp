//---------------------------------------------------------------------------


#pragma hdrstop

#include "PozycjaGrafiki.h"
#pragma package(smart_init)
//---------------------------------------------------------------------------
PozycjaGrafiki::PozycjaGrafiki()
{
    PunktX = 0;
    PunktY = 0;
}

PozycjaGrafiki::PozycjaGrafiki(int punktX, int punktY)
{
    PunktX = punktX;
    PunktY = punktY;
}

